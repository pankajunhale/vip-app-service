{
  "name": "vip-rest-service",
  "version": "1.0.0",
  "description": "VIP Purchase Order Processing Service",
  "main": "./src/app.ts",
  "scripts": {
    "start": "npx webpack --env development --env mode=development --config ./webpack.config.js && nodemon ./src/app.ts",
    "build": "npx webpack --env development --env mode=development --config ./webpack.config.js",
    "build-qa": "npx webpack --env qa --env mode=production --config ./webpack.config.qa.js",
    "build-prod": "npx webpack --env production --env mode=production --config ./webpack.config.prod.js",
    "start-watch": "nodemon --watch \"src/**\" --ext \"ts,json\" --ignore \"src/**/*.spec.ts\" --exec \"ts-node src/index.ts\"",
    "debug": "set DEBUG=* && npm start",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pankajunhale/vip-app-service.git"
  },
  "keywords": [
    "REST",
    "API",
    "ExpressJS",
    "NodeJS"
  ],
  "author": "VIP",
  "license": "MIT",
  "config": {
    "LOCAL_HOST_BASE_URL": "http://localhost:3030",
    "DEV_BASE_URL": "http://localhost:3030",
    "PROD_BASE_URL": "http://localhost:3030"
  },
  "dependencies": {
    "argon2": "^0.27.0",
    "cors": "^2.8.5",
    "debug": "^4.2.0",
    "dotenv": "^14.3.0",
    "express": "^4.17.1",
    "express-winston": "^4.0.5",
    "multer": "^1.4.4",
    "mysql": "^2.18.1",
    "node-cron": "^3.0.0",
    "shortid": "^2.2.16",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@types/argon2": "^0.15.0",
    "@types/cors": "^2.8.7",
    "@types/debug": "^4.1.5",
    "@types/express": "^4.17.2",
    "@types/mysql": "^2.15.19",
    "@types/node-cron": "^3.0.0",
    "@types/shortid": "0.0.29",
    "@types/underscore": "^1.11.3",
    "cross-env": "^7.0.3",
    "eslint": "^8.0.1",
    "gulp": "^4.0.2",
    "gulp-clean": "^0.4.0",
    "gulp-cli": "^2.3.0",
    "gulp-replace": "^1.1.3",
    "gulp-run": "^1.7.1",
    "html-loader": "^2.1.2",
    "html-webpack-plugin": "^5.4.0",
    "nodemon": "^2.0.12",
    "prettier": "^2.4.1",
    "source-map-support": "^0.5.16",
    "ts-loader": "^9.2.6",
    "ts-node": "^10.2.1",
    "tslint": "^6.0.0",
    "typescript": "^3.7.5",
    "underscore": "^1.13.1",
    "webpack": "^5.58.2",
    "webpack-cli": "^4.9.0",
    "webpack-dev-server": "^4.3.1",
    "webpack-merge": "^5.8.0",
    "webpack-node-externals": "^3.0.0"
  }
}
